<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.amanzi.awe.report.o2.GenerateReportCommand"
            name="Generate Report Command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.amanzi.awe.report.o2.handlers.GenerateReportCommandHandler"
            commandId="org.amanzi.awe.report.o2.GenerateReportCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.amanzi.awe.report.toolbar1">
            <command
                  commandId="org.amanzi.awe.report.o2.GenerateReportCommand"
                  id="org.amanzi.awe.report.o2.GenerateReportCommand"
                  label="Report"
                  tooltip="Generate Automatically Reports">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
