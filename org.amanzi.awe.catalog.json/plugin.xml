<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="org.amanzi.awe.catalog.json.ServiceExtensions"
         name="GeoJSON Service Extensions"
         point="net.refractions.udig.catalog.ServiceExtension">
      <service
            class="org.amanzi.awe.catalog.json.JSONServiceExtension"
            id="org.amanzi.awe.catalog.json.service"
            name="GeoJSON Catalog Service Extension">
      </service>
   </extension>
   <extension
         id="org.amanzi.awe.catalog.json.Formats"
         name="GeoJSON Formats"
         point="net.refractions.udig.catalog.ui.fileFormat">
      <fileService
            fileExtension="*.json,*.geo_json"
            name="GeoJSON (Javascript Object Notation for Geographic Data)">
      </fileService>
   </extension>

</plugin>
