<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.amanzi.loader.core.parser">
      <parser
            class="org.amanzi.awe.afp.ericsson.parser.NetworkConfigurationParser"
            id="org.amanzi.awe.afp.ericsson.parser.NetworkConfigurationParser">
      </parser>
      <parser
            class="org.amanzi.awe.afp.ericsson.parser.BarRirParser"
            id="org.amanzi.awe.afp.ericsson.parser.BarRirParser">
      </parser>
   </extension>
   <extension
         point="org.amanzi.loader.core.saver">
      <saver
            class="org.amanzi.awe.afp.ericsson.saver.NetworkConfigurationSaver"
            id="org.amanzi.awe.afp.ericsson.saver.NetworkConfigurationSaver">
      </saver>
      <saver
            class="org.amanzi.awe.afp.ericsson.saver.RecordSaver"
            id="org.amanzi.awe.afp.ericsson.saver.RecordSaver">
      </saver>
      <saver
            class="org.amanzi.awe.afp.ericsson.saver.BarRirSaver"
            id="org.amanzi.awe.afp.ericsson.saver.BarRirSaver">
      </saver>
   </extension>
   <extension
         point="org.amanzi.neo.loader.ui.loaders">
      <loader
            description="Ericsson Network config loader"
            gui="org.amanzi.neo.loader.ui.wizards.NetworkConfigurationImportWizard"
            parser="org.amanzi.awe.afp.ericsson.parser.NetworkConfigurationParser"
            saver="org.amanzi.awe.afp.ericsson.saver.NetworkConfigurationSaver"
            validator="org.amanzi.awe.afp.ericsson.ui.EricssonValidator">
         <pages
               class="org.amanzi.awe.afp.ericsson.ui.EricssonNetworkGui2">
         </pages>
      </loader>
      <loader
            description="Ericsson Network BAR-RIR loader"
            gui="org.amanzi.neo.loader.ui.wizards.NetworkMeasurementImportWizard"
            parser="org.amanzi.awe.afp.ericsson.parser.BarRirParser"
            saver="org.amanzi.awe.afp.ericsson.saver.BarRirSaver"
            validator="org.amanzi.awe.afp.ericsson.parser.RirValidator">
         <pages
               class="org.amanzi.awe.afp.ericsson.ui.RirSelectionGui">
         </pages>
      </loader>
   </extension>


</plugin>
