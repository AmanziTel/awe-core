<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Database Project Explorer"
            icon="icons/project.png"
            category="org.amanzi.awe.views"
            class="org.amanzi.awe.explorer.ui.view.ProjectExplorerView"
            id="org.amanzi.trees.ProjectExplorer">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.amanzi.awe.perspective">
         <view
               id="org.amanzi.trees.ProjectExplorer"
               minimized="false"
               ratio="0.35"
               relationship="top"
               relative="net.refractions.udig.project.ui.layerManager"
               visible="true">
         </view>
         <viewShortcut
               id="org.amanzi.trees.ProjectExplorer">
         </viewShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.amanzi.awe.explorer.ui.preferences.ExplorerLabelsInitialzer">
      </initializer>
   </extension>
   
</plugin>
