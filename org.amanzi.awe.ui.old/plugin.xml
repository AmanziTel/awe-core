<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.amanzi.awe.ui.UdigEventManager">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.amanzi.awe.ui.preference.AwePreferencePage"
            id="org.amanzi.awe.ui.preference.awe"
            name="Awe">
      </page>
      <page
            category="org.amanzi.awe.ui.preference.awe"
            class="org.amanzi.awe.ui.preference.NetworkPreferencePage"
            id="org.amanzi.awe.ui.preference.network"
            name="Network">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.amanzi.awe.ui.preference.NetworkPreferenceInitializer">
      </initializer>
   </extension>

</plugin>
