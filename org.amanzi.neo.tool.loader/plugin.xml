<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Neoclipse"
            id="org.amanzi.dalibor">
      </category>
      <view
            name="Neo tool view"
            icon="icons/32/network.png"
            category="org.amanzi.dalibor"
            class="org.amanzi.neo.loader.views.NeoToolView"
            id="org.amanzi.dalibor.views.SampleView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.TaskList"
               relationship="right"
               id="org.amanzi.dalibor.views.SampleView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="net.refractions.udig.project.ui.tool">
      <actionTool
            categoryId="Neo_tool"
            class="org.amanzi.neo.loader.tools.LoadNetwork"
            icon="icons/32/network.png"
            id="org.amanzi.neo.tool.loader.LoadNetwork"
            onToolbar="true"
            tooltip="Load network ">
         <enablement></enablement>
      </actionTool>
      <actionTool
            class="org.amanzi.neo.loader.tool.LoadTEMS"
            commandHandler="TEMSLoader"
            icon="icons/32/file.png"
            id="org.amanzi.neo.tool.loader.loadTems"
            onToolbar="true"
            tooltip="Load TEMS">
         <enablement></enablement>
      </actionTool>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="org.amanzi.neo.tool.loader.LoadNetworkContribution">
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.amanzi.neo.tool.loader.ViewPart1"
            id="org.amanzi.neo.tool.loader.view1"
            name="name">
      </view>
   </extension>

</plugin>
