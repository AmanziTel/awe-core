<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="Distribution"
            icon="icons/distribution.png"
            category="org.amanzi.awe.script"
            class="org.amanzi.awe.views.reuse.views.ReuseAnalyserView"
            id="org.amanzi.awe.views.reuse.views.ReuseAnalyserView">
      </view>
      <view
            name="Property Table"
            icon="icons/properties.gif"
            category="org.amanzi.awe.script"
            class="org.amanzi.awe.views.reuse.mess_table.view.MessageAndEventTableView"
            id="org.amanzi.awe.views.reuse.views.MessageAndEventTableView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.amanzi.awe.perspective">
         <view
               id="org.amanzi.awe.views.reuse.views.ReuseAnalyserView"
               relationship="stack"
               relative="net.refractions.udig.catalog.ui.CatalogView"
               visible="true">
         </view>
         <viewShortcut
               id="org.amanzi.awe.views.reuse.views.ReuseAnalyserView">
         </viewShortcut>
         <view
               id="org.amanzi.awe.views.reuse.views.MessageAndEventTableView"
               relationship="stack"
               relative="net.refractions.udig.catalog.ui.CatalogView"
               visible="true">
         </view>
         <viewShortcut
               id="org.amanzi.awe.views.reuse.views.MessageAndEventTableView">
         </viewShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.amanzi.afp.perspective">
         <view
               id="org.amanzi.awe.views.reuse.views.ReuseAnalyserView"
               relationship="stack"
               relative="net.refractions.udig.catalog.ui.CatalogView"
               visible="true">
         </view>
         <viewShortcut
               id="org.amanzi.awe.views.reuse.views.ReuseAnalyserView">
         </viewShortcut>
         <view
               id="org.amanzi.awe.views.reuse.views.MessageAndEventTableView"
               relationship="stack"
               relative="net.refractions.udig.catalog.ui.CatalogView"
               visible="true">
         </view>
         <viewShortcut
               id="org.amanzi.awe.views.reuse.views.MessageAndEventTableView">
         </viewShortcut>
      </perspectiveExtension>
   </extension>

</plugin>
