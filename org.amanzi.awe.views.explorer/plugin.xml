<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Database Project Explorer"
            icon="icons/project.png"
            category="org.amanzi.awe.views"
            class="org.amanzi.awe.views.explorer.view.ProjectExplorerView"
            id="org.amanzi.awe.views.explorer.view.ProjectExplorerView">
      </view>
      
   </extension>
   <extension
          point= "org.eclipse.ui.menus" >
       <menuContribution
             locationURI="menu:org.eclipse.ui.main.menu">
          <menu
                id= "file"
                label= "File" >
             <menu
                   id="ChangeActiveProject"
                   label="Change active project">
                <dynamic
                      class="org.amanzi.awe.views.explorer.contributions.ProjectSelectionMenu"
                      id="org.amanzi.awe.views.explorer.contributions.ProjectSelectionMenu">
                </dynamic>
             </menu>
          </menu>
       </menuContribution>
    </extension>
    
         
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.amanzi.awe.views.explorer.newproject.NewProjectWizardInitHandler"
            id="org.amanzi.awe.views.explorer.newproject.NewProjectWizardInitHandler"
            name="New Project">
      </command>
   </extension>


	<extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.amanzi.awe.views.explorer.view.ProjectExplorerView">
         <command         		
               commandId="org.amanzi.awe.views.explorer.newproject.NewProjectWizardInitHandler"
               icon="icons/project.png"
               label="New Project"
               style="push">
         </command>
      </menuContribution>
   </extension>
    
</plugin>
