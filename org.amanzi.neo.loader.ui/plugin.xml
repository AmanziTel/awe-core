                   tooltip="Load Network Configuration Data">
                <parameter
                      name="org.amanzi.neo.loader.ui.commands.guiId"
                      value="org.amanzi.neo.loader.ui.wizards.NetworkConfigurationImportWizard">
                </parameter>
             </command>
             <command
                   commandId="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   icon="icons/16/oss_stats.png"
                   id="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   style="push"
                   tooltip="Load Network Measurements Data">
                <parameter
                      name="org.amanzi.neo.loader.ui.commands.guiId"
                      value="org.amanzi.neo.loader.ui.wizards.NetworkMeasurementImportWizard">
                </parameter>
             </command>
          </toolbar>
