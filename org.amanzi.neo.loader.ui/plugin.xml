      <extension
          point="org.eclipse.ui.commands">
       <category
             id="org.amanzi.neo.loader.commands.category"
             name="Amanzi Loading Commands">
       </category>         
       <command
             categoryId="org.amanzi.neo.loader.commands.category"
             id="org.amanzi.neo.loader.ui.commands.LoadGPEHHandler"
             name="Load OSS Data">
                       <commandParameter id="org.amanzi.neo.loader.commands.addtoselect" name="add to select" optional="true">
        </commandParameter>
       </command>
       <command
             categoryId="org.amanzi.neo.loader.commands.category"
             id="org.amanzi.neo.loader.ui.commands.LaunchLoader"
             name="LoadLauncher">
          <commandParameter
                id="org.amanzi.neo.loader.ui.commands.guiId"
                name="gui id"
                optional="false">
          </commandParameter>
       </command>
       <command
             categoryId="org.amanzi.neo.loader.commands.category"
             id="org.amanzi.neo.loader.ui.commands.LaunchLoaderNew"
             name="LaunchLoaderNew">
          <commandParameter
                id="org.amanzi.neo.loader.ui.commands.guiId"
                name="gui id"
                optional="false">
          </commandParameter>
       </command>   
       
    </extension>
    <extension
          point="org.eclipse.ui.handlers">       
       <handler
             class="org.amanzi.neo.loader.ui.handlers.LaunchLoader"
             commandId="org.amanzi.neo.loader.ui.commands.LaunchLoader">
       </handler>
    </extension>
   <extension
          point="org.eclipse.ui.handlers">       
       <handler
             class="org.amanzi.neo.loader.ui.handlers.LaunchLoaderNew"
             commandId="org.amanzi.neo.loader.ui.commands.LaunchLoaderNew">
       </handler>
    </extension>
    
    <extension
