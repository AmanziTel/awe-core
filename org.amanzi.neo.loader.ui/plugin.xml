       <menuContribution
             locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
          <toolbar
                id="org.amanzi.neo.loader.toolbar2">             
           
              <command
                   commandId="org.amanzi.neo.loader.ui.commands.LaunchLoaderNew"
                   icon="icons/16/network.png"
                   id="org.amanzi.neo.loader.ui.commands.LaunchLoaderNew"
                   tooltip="Load Network Data">
                <parameter
                      name="org.amanzi.neo.loader.ui.commands.guiId"
                      value="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard">
                </parameter>
             </command>
             <command
                   commandId="org.amanzi.neo.loader.ui.commands.LaunchLoaderNew"
                   icon="icons/16/dataset.png"
                   id="org.amanzi.neo.loader.ui.LoadLauncher"
                   tooltip="Load Dataset">
                <parameter
                      name="org.amanzi.neo.loader.ui.commands.guiId"
                      value="org.amanzi.neo.loader.ui.wizards.DatasetImportWizard">
                </parameter>
             </command>            
             <command
                   commandId="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   icon="icons/16/oss.png"
                   id="org.amanzi.neo.loader.ui.LoadLauncher"
                   tooltip="Load OSS Data">
                <parameter
                      name="org.amanzi.neo.loader.ui.commands.guiId"
                      value="org.amanzi.neo.loader.ui.wizards.OssImportWizard">
                </parameter>
             </command>
             <command
                   commandId="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   icon="icons/16/oss.png"
                   id="org.amanzi.neo.loader.ui.commands.LaunchLoader"
