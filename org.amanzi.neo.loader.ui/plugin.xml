             id="NewNetworkLoader"
             loader_class="org.amanzi.neo.loader.ui.loaders.SimpleLoader"
             loader_data_type="Network"
             loader_name="NewNetworkLoader"
             loader_type="site sector loader"
             parser="org.amanzi.neo.loader.core.newparser.CommonCSVParser"
             validator="org.amanzi.neo.loader.ui.validators.NetworkValidator">
               <saver 
               id="org.amanzi.neo.loader.core.newsaver.NetworkSaver"
            	class="org.amanzi.neo.loader.core.newsaver.NetworkSaver"
            />
      </newloader>
     
      <newloader
