<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.amanzi.loader.core.parser" name="Extension for new Parsers" schema="schema/org.amanzi.loader.core.parser.exsd"/>
   <extension-point id="org.amanzi.loader.core.saver" name="Extension for new Saver" schema="schema/org.amanzi.loader.core.saver.exsd"/>
      <!-- new parsers config -->
    <extension
         point="org.amanzi.loader.core.parser">
      <parser
            class="org.amanzi.neo.loader.core.parser.CommonCSVParser"
            id="org.amanzi.neo.loader.core.parser.CommonCSVParser">
      </parser>
   </extension>
   <!-- new savers config -->
   <extension
         point="org.amanzi.loader.core.saver">
      <saver
            class="org.amanzi.neo.loader.core.saver.NetworkSaver"
            id="org.amanzi.neo.loader.core.saver.NetworkSaver">
      </saver>
       
      <saver
            class="org.amanzi.neo.loader.core.saver.SectorSelectionSaver"
            id="org.amanzi.neo.loader.core.saver.SectorSelectionSaver">
      </saver>
       
      <saver
            class="org.amanzi.neo.loader.core.saver.NeighborsSaver"
            id="org.amanzi.neo.loader.core.saver.NeighborsSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.TemsSaver"
            id="org.amanzi.neo.loader.core.saver.TemsSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.RomesSaver"
            id="org.amanzi.neo.loader.core.saver.RomesSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.Nemo1xSaver"
            id="org.amanzi.neo.loader.core.saver.Nemo1xSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.Nemo2xSaver"
            id="org.amanzi.neo.loader.core.saver.Nemo2xSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.TRXSaver"
            id="org.amanzi.neo.loader.core.saver.TRXSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.TrafficSaver"
            id="org.amanzi.neo.loader.core.saver.TrafficSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.SeparationCostraintSaver"
            id="org.amanzi.neo.loader.core.saver.SeparationCostraintSaver">
      </saver>
      
       <saver
            class="org.amanzi.neo.loader.core.saver.FrequencyConstraintSaver"
            id="org.amanzi.neo.loader.core.saver.FrequencyConstraintSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.InterferenceSaver"
            id="org.amanzi.neo.loader.core.saver.InterferenceSaver">
      </saver>
   </extension>

</plugin>
