<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.amanzi.loader.core.parser" name="Extension for new Parsers" schema="schema/org.amanzi.loader.core.parser.exsd"/>
   <extension-point id="org.amanzi.loader.core.saver" name="Extension for new Saver" schema="schema/org.amanzi.loader.core.saver.exsd"/>
   <extension-point id="org.amanzi.loader.core.validator" name="Validator for Loaders" schema="schema/org.amanzi.loader.core.validator.exsd"/>
      <!-- new parsers config -->
    <extension
         point="org.amanzi.loader.core.parser">
      <parser
            class="org.amanzi.neo.loader.core.parser.CommonCSVParser"
            id="org.amanzi.neo.loader.core.parser.CommonCSVParser">
      </parser>
   </extension>
   <!-- new savers config -->
   <extension
         point="org.amanzi.loader.core.saver">
      <saver
            class="org.amanzi.neo.loader.core.saver.NetworkSaver"
            id="org.amanzi.neo.loader.core.saver.NetworkSaver">
      </saver>
       
       
      <saver
            class="org.amanzi.neo.loader.core.saver.NeighboursSaver"
            id="org.amanzi.neo.loader.core.saver.NeighboursSaver">
      </saver>
      
      <saver
            class="org.amanzi.neo.loader.core.saver.InterferenceMatrixSaver"
            id="org.amanzi.neo.loader.core.saver.InterferenceMatrixSaver">
      </saver>
   </extension>
   
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.amanzi.neo.loader.core.preferences.SynonymsInitializer">
      </initializer>
   </extension>

</plugin>
