<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="Network Tree"
            icon="icons/network.png"
            category="org.amanzi.awe.script"
            class="org.amanzi.awe.views.network.view.NetworkTreeView"
            id="org.amanzi.awe.views.network.views.NetworkTreeView">
      </view>
      
   </extension>
   <extension
            point="org.eclipse.ui.viewActions">
         <viewContribution
               targetID="org.amanzi.awe.views.network.views.NetworkTreeView"
               id="org.amanzi.awe.views.network.view.NewTypeAction0">
            <action
                  toolbarPath="additions"
                  label="Label"
                  class="org.amanzi.awe.views.network.view.NewTypeAction"
                  icon="icons/add-type.gif"
                  tooltip="Create new node type"
                  style="push"
                  id="org.amanzi.awe.views.network.view.NewTypeAction">
               <!--<enablement>
                  <objectClass name="net.refractions.udig.project.ILayer"/>
               </enablement>!-->
            </action>
            
         </viewContribution>
      </extension>

</plugin>
