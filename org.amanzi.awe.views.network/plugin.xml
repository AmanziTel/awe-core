<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.amanzi.awe.views.tree.handlers" name="Tree Handler" schema="schema/org.amanzi.awe.views.tree.handlers.exsd"/>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="Network Tree"
            icon="icons/network.png"
            category="org.amanzi.awe.script"
            class="org.amanzi.awe.views.network.view.NewNetworkTreeView"
            id="org.amanzi.awe.views.network.views.NewNetworkTreeView">
      </view>
      
   </extension>
   <extension
            point="org.eclipse.ui.viewActions">
         <viewContribution
               targetID="org.amanzi.awe.views.network.views.NewNetworkTreeView"
               id="org.amanzi.awe.views.network.view.NewTypeAction0">
            <action
                  toolbarPath="additions"
                  label="Label"
                  class="org.amanzi.awe.views.network.view.NewTypeAction"
                  icon="icons/add-type.gif"
                  tooltip="Create new node type"
                  style="push"
                  id="org.amanzi.awe.views.network.view.NewTypeAction">

            </action>
            <action
                  class="org.amanzi.awe.views.network.view.AddToSelectionList"
                  icon="icons/add-selectionList.gif"
                  id="org.amanzi.awe.views.network.NewSelectionList"
                  label="Label"
                  style="push"
                  toolbarPath="additions"
                  tooltip="Create new selection list">
            </action>
            
         </viewContribution>
      </extension>
</plugin>
