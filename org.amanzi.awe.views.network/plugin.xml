<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Network Tree"
            icon="icons/network.png"
            category="org.amanzi.awe.views"
            class="org.amanzi.awe.views.network.view.NetworkTreeView"
            id="org.amanzi.awe.views.network.views.NewNetworkTreeView">
      </view>
   </extension>
   
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.amanzi.awe.perspective">
         <view
               id="org.amanzi.awe.views.network.views.NewNetworkTreeView"
               relationship="stack"
               relative="net.refractions.udig.catalog.ui.CatalogView"
               visible="true">
         </view>
         <viewShortcut
               id="org.amanzi.awe.views.network.views.NewNetworkTreeView">
         </viewShortcut>
      </perspectiveExtension>
   </extension>
   
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Network Properties View"
            icon="icons/network.png"
            category="org.amanzi.awe.views"
            class="org.amanzi.awe.views.network.view.NetworkPropertiesView"
            id="org.amanzi.awe.views.network.views.NetworkPropertiesView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.amanzi.awe.views.network.view.CopyHandler"
            id="org.amanzi.awe.views.network.command.Copy"
            name="Copy">
      </command>
      <command
            defaultHandler="org.amanzi.awe.views.network.view.FilterHandler"
            id="org.amanzi.awe.views.network.command.Filter"
            name="Filter">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.amanzi.awe.views.network.views.NetworkPropertiesView">
         <command
               commandId="org.amanzi.awe.views.network.command.Copy"
               icon="icons/copy.png"
               label="Copy content"
               style="push">
         </command>
         <command
               commandId="org.amanzi.awe.views.network.command.Filter"
               icon="icons/filter.png"
               label="Filter"
               style="push">
         </command>
      </menuContribution>
   </extension>
   

   
</plugin>
