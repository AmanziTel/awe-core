<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.amanzi.awe.wizards.KPIReportCommand"
            name="KPI Report">
      </command>
      <command
      		id="org.amanzi.awe.wizards.Correlation"
      		name="Network Correlation">
      </command>
      <command
            id="org.amanzi.awe.wizards.NetViewCommand"
            name="Net View">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.amanzi.awe.wizards.kpi.report.KPIReportHandler"
            commandId="org.amanzi.awe.wizards.KPIReportCommand">
      </handler>
      <handler
      		class="org.amanzi.awe.wizards.geoptima.GeoptimaHandler"
      		commandId="org.amanzi.awe.wizards.Correlation">
      </handler>
      <handler
            class="org.amanzi.awe.wizards.netview.NetViewHandler"
            commandId="org.amanzi.awe.wizards.NetViewCommand">
      </handler>
      		
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.amanzi.awe.report.toolbar1">
            <command
                  commandId="org.amanzi.awe.wizards.KPIReportCommand"
                  id="org.amanzi.awe.wizards.KPIReportCommand"
                  label="KPI Report"
                  mnemonic="KPI Report">
            </command>
            <command
            	  commandId="org.amanzi.awe.wizards.Correlation"
            	  id="org.amanzi.awe.wizards.Correlation"
            	  label="Network Correlation"
            	  mnemonic="Network Correlation">
            </command>
            <command
                  commandId="org.amanzi.awe.wizards.NetViewCommand"
                  id="org.amanzi.awe.wizards.NetViewCommand"
                  label="Net View"
                  mnemonic="Net View">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
