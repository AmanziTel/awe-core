      </handler>
      <handler
      		class="org.amanzi.awe.wizards.geoptima.VisualiseHandler"
      		commandId="org.amanzi.awe.wizards.geoptima.visualise">
      </handler>
      <handler
            class="org.amanzi.awe.wizards.netview.NetViewHandler"
            commandId="org.amanzi.awe.wizards.NetViewCommand">
      </handler>
      		
