      		commandId="org.amanzi.awe.wizards.Correlation">
      </handler>
      <handler
      		class="org.amanzi.awe.wizards.geoptima.SelectDataSourceHandler"
      		commandId="org.amanzi.awe.wizards.geoptima.select">
      </handler>
      <handler
      		class="org.amanzi.awe.wizards.geoptima.GeoptimaHandler"
      		commandId="org.amanzi.awe.wizards.geoptima.load">
