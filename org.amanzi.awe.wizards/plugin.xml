         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.amanzi.awe.report.toolbar1">
            <command
                  commandId="org.amanzi.awe.wizards.KPIReportCommand"
                  id="org.amanzi.awe.wizards.KPIReportCommand"
                  label="KPI Report"
                  mnemonic="KPI Report">
            </command>
            <command
            	  commandId="org.amanzi.awe.wizards.Correlation"
            	  id="org.amanzi.awe.wizards.Correlation"
            	  style="pulldown" 
            	  label="GEOptimA"
            	  mnemonic="GEOptimA">
            	  
            </command>


            <command
                  commandId="org.amanzi.awe.wizards.NetViewCommand"
                  id="org.amanzi.awe.wizards.NetViewCommand"
                  label="Net View"
                  mnemonic="Net View">
