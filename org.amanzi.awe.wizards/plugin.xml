             <command
                   commandId="org.amanzi.neo.loader.commands.LoadNetworkSite"
            icon="icons/16/network_site.png"
            id="org.amanzi.neo.loader.LoadNetworkSite.level2"
             style="push"
                   tooltip="Load Network Data"
                   label="Load Network Data"
                   mnemonic="Load Network Data">
           <parameter name="org.amanzi.neo.loader.commands.addtoselect" value="true">
           </parameter>
             </command>
             <command
                   commandId="org.amanzi.neo.loader.commands.LoadGPEHHandler"
            icon="icons/16/delta_network.png"
            id="org.amanzi.neo.loader.LoadGPEH.level2"
             style="push"
                   tooltip="Load OSS Data">
                        <parameter name="org.amanzi.neo.loader.commands.addtoselect" value="true">
           </parameter>
             </command>
             <command
                   commandId="org.amanzi.neo.loader.commands.LoadDrive"
            icon="icons/16/drive.png"
            id="org.amanzi.neo.loader.LoadDrive.level2"
             style="push"
                   tooltip="Load Drive Data">
             <parameter name="org.amanzi.neo.loader.commands.addtoselect" value="true">
           </parameter>
             </command>
             
