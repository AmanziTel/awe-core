<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.amanzi.neo.loader.ui.gui" name="GUI for loaders" schema="schema/org.amanzi.neo.loader.ui.gui.exsd"/>
   <extension-point id="org.amanzi.neo.loader.ui.loaders" name="Loaders" schema="schema/org.amanzi.neo.loader.ui.loaders.exsd"/>
      <extension
          point="org.eclipse.ui.commands">
       <category
             id="org.amanzi.neo.loader.commands.category"
             name="Amanzi Loading Commands">
       </category>         
       <command
             categoryId="org.amanzi.neo.loader.commands.category"
             id="org.amanzi.neo.loader.ui.commands.LoadGPEHHandler"
             name="Load OSS Data">
                       <commandParameter id="org.amanzi.neo.loader.commands.addtoselect" name="add to select" optional="true">
        </commandParameter>
       </command>
       <command
             categoryId="org.amanzi.neo.loader.commands.category"
             id="org.amanzi.neo.loader.ui.commands.LaunchLoader"
             name="LoadLauncher">
          <commandParameter
                id="org.amanzi.neo.loader.ui.commands.guiId"
                name="gui id"
                optional="false">
          </commandParameter>
       </command>   
    </extension>
    <extension
          point="org.eclipse.ui.handlers">       
       <handler
             class="org.amanzi.neo.loader.ui.handlers.LaunchLoader"
             commandId="org.amanzi.neo.loader.ui.commands.LaunchLoader">
       </handler>
    </extension>
   
    <extension
          point="org.eclipse.ui.menus">
       <menuContribution
             locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
          <toolbar
                id="org.amanzi.neo.loader.toolbar2">             
             <command
                   commandId="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   icon="icons/16/network.png"
                   id="org.amanzi.neo.loader.ui.LoadLauncher"
                   tooltip="Load Network Data">
                <parameter
                      name="org.amanzi.neo.loader.ui.commands.guiId"
                      value="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard">
                </parameter>
             </command>     
             <command
                   commandId="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   icon="icons/16/oss.png"
                   id="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   style="push"
                   tooltip="Load Network Configuration Data">
                <parameter
                      name="org.amanzi.neo.loader.ui.commands.guiId"
                      value="org.amanzi.neo.loader.ui.wizards.NetworkConfigurationImportWizard">
                </parameter>
             </command>
             <command
                   commandId="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   icon="icons/16/oss_stats.png"
                   id="org.amanzi.neo.loader.ui.commands.LaunchLoader"
                   style="push"
                   tooltip="Load Network Measurements Data">
                <parameter
                      name="org.amanzi.neo.loader.ui.commands.guiId"
                      value="org.amanzi.neo.loader.ui.wizards.NetworkMeasurementImportWizard">
                </parameter>
             </command>        
          </toolbar>
       </menuContribution>
    </extension>
    <extension
          point="org.amanzi.neo.loader.ui.gui">       
       <GUI
             class="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             id="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard">
       </GUI>   
       <GUI
             class="org.amanzi.neo.loader.ui.wizards.NetworkConfigurationImportWizard"
             id="org.amanzi.neo.loader.ui.wizards.NetworkConfigurationImportWizard">
       </GUI>
       <GUI
             class="org.amanzi.neo.loader.ui.wizards.NetworkMeasurementImportWizard"
             id="org.amanzi.neo.loader.ui.wizards.NetworkMeasurementImportWizard">
       </GUI>    
    </extension>
    <extension
          point="org.amanzi.neo.loader.ui.loaders">
       <loader
             description="Network Sector Data"
			 config="org.amanzi.neo.loader.ui.validators.NetworkSectorDataValidator"
             gui="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             parser="org.amanzi.neo.loader.core.parser.CSVParser"
             saver="org.amanzi.neo.loader.core.saver.impl.NetworkSaver"
             validator="org.amanzi.neo.loader.ui.validators.NetworkSectorDataValidator"
             renderable="true"
             allowCreate="true">
       </loader>
       <loader
             description="Neighbour Loader"
             gui="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             parser="org.amanzi.neo.loader.core.parser.CSVParser"
             saver="org.amanzi.neo.loader.core.saver.impl.NeighbourSaver"
             validator="org.amanzi.neo.loader.ui.validators.NeighbourDataValidator">
       </loader>
       <loader
             description="Network Selection"
             gui="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             parser="org.amanzi.neo.loader.core.parser.LineParser"
             saver="org.amanzi.neo.loader.core.saver.impl.NetworkSelectionSaver"
             validator="org.amanzi.neo.loader.ui.validators.ExistNetworkValidator">
       </loader>
       <loader
             description="Traffic data"
             gui="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             saver="org.amanzi.neo.loader.core.saver.network.TrafficDataSaver"
             validator="org.amanzi.neo.loader.ui.validators.TrafficDataValidator"
             parser="org.amanzi.neo.loader.core.parser.CSVParser">
       </loader>
       <loader
             description="Frequency constraints data"
             gui="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             saver="org.amanzi.neo.loader.core.saver.network.FrequencyConstraintsSaver"
             validator="org.amanzi.neo.loader.ui.validators.FrequencyConstraintsDataValidator"
             parser="org.amanzi.neo.loader.core.parser.CSVParser">
       </loader>
	   <loader
             description="Separation constraints data"
             gui="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             saver="org.amanzi.neo.loader.core.saver.network.SeparationConstraintsSaver"
             validator="org.amanzi.neo.loader.ui.validators.SeparationConstraintsDataValidator"
             parser="org.amanzi.neo.loader.core.parser.CSVParser">
       </loader>
       <loader
             description="Interference Matrix"
             gui="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             parser="org.amanzi.neo.loader.core.parser.CSVParser"
             validator="org.amanzi.neo.loader.ui.validators.InterferenceMatrixValidator"
             saver="org.amanzi.neo.loader.core.saver.network.InterferenceMatrixSaver">
       </loader>
 	   <loader
             description="Trx data"
             gui="org.amanzi.neo.loader.ui.wizards.NetworkImportWizard"
             saver="org.amanzi.neo.loader.core.saver.network.TrxSaver"
             validator="org.amanzi.neo.loader.ui.validators.TrxDataValidator"
             parser="org.amanzi.neo.loader.core.parser.CSVParser">
       </loader>
    </extension>

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.amanzi.neo.loader.ui.preferences.DataLoadPreferencePage"
            id="org.amanzi.neo.loader.ui.preferences.DataLoadPreferencePage"
            name="Amanzi Data">
      </page>
      <page
      		class="org.amanzi.neo.loader.ui.preferences.SynonymLoaderPreferencePage"
      		id="org.amanzi.neo.loader.ui.preferences.SynonymsPreferencePage"
      		name="Synonyms"
      		category="org.amanzi.neo.loader.ui.preferences.DataLoadPreferencePage">
      </page>
       <page
            class="org.amanzi.neo.loader.ui.preferences.NetworkLoaderPreferencePage"
            id="org.amanzi.neo.loader.ui.preferences.NetworkLoaderPreferencePage"
            name="Network data"
            category="org.amanzi.neo.loader.ui.preferences.SynonymsPreferencePage">
      </page>
      <page
            class="org.amanzi.neo.loader.ui.preferences.DriveLoaderPreferencePage"
            id="org.amanzi.neo.loader.ui.preferences.DriveLoaderPreferencePage"
            name="Drive data"
            category="org.amanzi.neo.loader.ui.preferences.SynonymsPreferencePage">
      </page>
      <page
            class="org.amanzi.neo.loader.ui.preferences.CommonCRSPreferencePage"
            id="org.amanzi.neo.loader.ui.preferences.CommonCRSPreferencePage"
            name="Common CRS"
            category="org.amanzi.neo.loader.ui.preferences.DataLoadPreferencePage">
      </page>

	  <page
            class="org.amanzi.neo.loader.ui.preferences.NeighbourLoaderPreferencePage"
            id="org.amanzi.neo.loader.ui.preferences.NeighbourLoaderPreferencePage"
            name="Neighbour lists"
            category="org.amanzi.neo.loader.ui.preferences.SynonymsPreferencePage">
      </page>
      <page
            class="org.amanzi.neo.loader.ui.preferences.ProbeLoaderPreferencePage"
            id="org.amanzi.neo.loader.ui.preferences.ProbeLoaderPreferencePage"
            name="Probe data"
            category="org.amanzi.neo.loader.ui.preferences.SynonymsPreferencePage">
      </page>
      <page
            class="org.amanzi.neo.loader.ui.preferences.TransmissionLoaderPreferencePage"
            id="org.amanzi.neo.loader.ui.preferences.TransmissionLoaderPreferencePage"
            name="Transmission lists"
            category="org.amanzi.neo.loader.ui.preferences.SynonymsPreferencePage">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.amanzi.neo.loader.ui.preferences.DataLoadPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.amanzi.neo.loader.ui.loaders">
   </extension>
</plugin>
