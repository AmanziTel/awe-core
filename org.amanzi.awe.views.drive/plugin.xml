<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="Data Tree"
            icon="icons/driveTree.gif"
            category="org.amanzi.awe.script"
            class="org.amanzi.awe.views.drive.views.DriveTreeView"
            id="org.amanzi.awe.views.drive.views.DriveTreeView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.amanzi.awe.perspective">
         <view
               ratio="0.5"
               relative="org.amanzi.awe.networktree.views.NetworkTreeView"
               relationship="stack"
               id="org.amanzi.awe.views.drive.views.DriveTreeView">
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
