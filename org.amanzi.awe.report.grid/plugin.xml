<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.amanzi.awe.report.grid.CreateReportsCommand"
            name="Grid Report">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.amanzi.awe.report.grid.handlers.GridReportCommandHandler"
            commandId="org.amanzi.awe.report.grid.CreateReportsCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.amanzi.awe.report.grid.toolbar1">
            <command
                  commandId="org.amanzi.awe.report.grid.CreateReportsCommand"
                  label="Grid report">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
